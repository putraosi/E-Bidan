{
  "name": "cikal_mulia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-release": "npx react-native run-android --variant=release",
    "ios": "npx react-native run-ios;",
    "start": "npx react-native start --reset-cache",
    "link": "npx react-native link",
    "lint": "eslint .",
    "assembleRelease": "cd android; ./gradlew clean; ./gradlew assembleRelease",
    "bundleRelease": "cd android; ./gradlew bundleRelease",
    "bundle": "react-native bundle --verbose --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "apk-debug": "yarn bundle; cd android; ./gradlew clean; ./gradlew assembleDebug",
    "apk-release": "yarn; node android-react-gradle-fix.js; yarn assembleRelease",
    "update-nm": "rm -rf node_modules yarn.lock; rm -rf /tmp/metro-bundler-cache-*; rm -rf /tmp/haste-map-react-native-packager-*; yarn"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "axios": "^0.24.0",
    "moment": "^2.29.1",
    "qs": "^6.10.2",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dash": "^0.0.11",
    "react-native-device-info": "^8.4.8",
    "react-native-image-picker": "^4.6.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-permissions": "^3.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-simple-toast": "^1.1.3",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.4.5",
    "eslint": "8.6.0",
    "jest": "^27.4.5",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.12",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
